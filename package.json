{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "concurrently \"npx vite\" \"npm run tailwind-dev\"",
    "dev": "concurrently \"npx vite\" \"npm run tailwind-dev\"",
    "build": "vite build",
    "serve": "vite preview",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./dist/output.css",
    "tailwind-dev": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.185",
    "concurrently": "^7.4.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0"
  },
  "dependencies": {
    "daisyui": "^2.27.0",
    "lodash": "^4.17.21",
    "solid-js": "^1.5.1"
  }
}
